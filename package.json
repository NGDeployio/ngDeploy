{
  "name": "ngdeploy",
  "description": "Generate a command line application for node.js.",
  "version": "0.1.7",
  "homepage": "https://github.com/mbejda/ngdeploy",
  "author": "Milos Bejda (https://github.com/mbejda)",
  "repository": {
    "type": "git",
    "url": "git://github.com/mbejda/ngdeploy.git"
  },
  "bugs": {
    "url": "https://github.com/mbejda/ngdeploy/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mbejda/ngdeploy/blob/master/LICENSE-MIT"
    }
  ],
  "keywords": [
    "cli",
    "command line",
    "deploy",
    "angular"
  ],
  "preferGlobal": true,
  "bin": {
    "ngdeploy": "./bin/ngdeploy"
  },
  "main": "index.js",
  "engines": {
    "node": ">=0.8"
  },
  "scripts": {
    "push-fury": "fury push ngdeploy-*.tgz",
    "commit-git": "git add . && git commit -m 'version upping' && git push",
    "patch-release": "npm version patch && npm pack",
    "build": "npm run commit-git && npm run patch-release && npm run push-fury",
    "test": "mocha"
  },
  "dependencies": {
    "async": "^1.5.2",
    "chalk": "^1.1.1",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "commander-plus": "0.0.6",
    "glob": "^7.0.3",
    "inquirer": "^0.12.0",
    "level": "^1.3.0",
    "minimist": "~0.0.8",
    "progress": "^1.1.8",
    "prompt": "^1.0.0",
    "readdirp": "^2.0.0",
    "request": "^2.69.0",
    "s3": "^4.4.0",
    "single-line-log": "^1.1.1",
    "superagent": "^1.8.1",
    "sync-request": "^3.0.0",
    "verbalize": "~0.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "grunt-strip-code": "^1.0.4",
    "verb": "~0.2.0"
  },
  "license": "ISC"
}
